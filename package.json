{
  "name": "scryfall-cache",
  "description": "Finding massive amounts of cards on scryfall without overloading the api",
  "version": "0.3.1",
  "license": "ISC",
  "main": "dist/index",
  "repository": {
    "type": "git",
    "url": "git@github.com:princejonn/scryfall-cache.git"
  },
  "bugs": {
    "url": "https://github.com/princejonn/scryfall-cache/issues"
  },
  "author": "Jonn Nilsson",
  "maintainers": [
    "jonn@jonn.nu"
  ],
  "scripts": {
    "build": "tsc",
    "check": "npm-check --update",
    "clean": "rimraf dist && rimraf coverage",
    "coverage": "jest --coverage",
    "lint": "eslint --ext .ts ./src/",
    "lint:fix": "eslint --ext .ts ./src/ --fix",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run clean && npm run build",
    "preversion": "npm run lint && npm run coverage",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "camelcase-keys": "^6.1.1",
    "date-fns": "^2.8.1",
    "download": "^7.1.0",
    "lodash": "^4.17.15",
    "simple-winston": "^0.1.0"
  },
  "devDependencies": {
    "@types/download": "^6.2.4",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "npm-check": "^5.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  }
}
