{
  "name": "scryfall-cache",
  "version": "0.2.1",
  "author": "jonn@jonn.nu",
  "description": "Finding massive amounts of cards on scryfall without overloading the api",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:princejonn/scryfall-cache.git"
  },
  "main": "lib/index",
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "check": "npm-check --update",
    "clean": "rimraf lib",
    "clean:modules": "npm run clean && rimraf package-lock.json && rimraf node_modules",
    "clean:install": "npm run clean:modules && npm install && git add package-lock.json",
    "lint": "eslint --ext .js ./src/",
    "lint:fix": "eslint --ext .js ./src/ --fix",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run clean && npm run lint && npm run build && npm test",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "test": "jest"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "date-fns-timezone": "^0.1.4",
    "download": "^7.1.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.15",
    "lodash-humps": "^3.1.5",
    "query-string": "^6.8.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.3",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0"
  }
}
